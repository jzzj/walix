{
  "manifest_version": 2,
  "name": "Walix",
  "description": "Walix - give you a hand",
  "version": "1.0",
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Walix"
  },
  "minimum_chrome_version": "45.0",
  "devtools_page": "devtools.html",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}